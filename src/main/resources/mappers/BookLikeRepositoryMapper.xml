<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Recorders.ggogit.domain.book.repository.BookLikeRepository">
    <insert id="save" parameterType="Recorders.ggogit.domain.book.entity.BookLike">
        INSERT INTO BOOK_LIKE (MEMBER_ID, BOOK_ID, ACTIVATE)
        VALUES (#{memberId}, #{bookId}, #{activate})
    </insert>
</mapper>


