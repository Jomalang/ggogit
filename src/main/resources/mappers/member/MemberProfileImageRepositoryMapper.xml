<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Recorders.ggogit.domain.member.repository.MemberProfileImageRepository">
    <insert id="save" parameterType="MemberProfileImage">
        INSERT INTO MEMBER_PROFILE_IMAGE(MEMBER_ID, NAME)
        VALUES(#{memberId}, #{name})
    </insert>

</mapper>